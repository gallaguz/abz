{
    "name": "abz-api",
    "version": "0.0.1",
    "description": "Node.js Developer Test Assignment",
    "author": "Andrii Hallahuz",
    "prisma": {
        "seed": "npx ts-node src/database/prisma/seed.ts"
    },
    "scripts": {
        "start": "node -r dotenv/config ./dist/main.js",
        "build": "tsc",
        "seed:dev": "npx ts-node src/database/prisma/seed.ts",
        "seed:deploy": "npx prisma db seed",
        "dev": "npx nodemon -e ts,json --exec node -r ts-node/register src/main.ts",
        "generate": "npx prisma generate --schema src/database/prisma/schema.prisma",
        "migrate:dev": "npx prisma migrate dev --schema src/database/prisma/schema.prisma",
        "migrate:deploy": "npx prisma migrate deploy --schema src/database/prisma/schema.prisma",
        "migrate:reset": "npx prisma migrate reset --schema src/database/prisma/schema.prisma"
    },
    "dependencies": {
        "@prisma/client": "5.14.0",
        "axios": "1.7.1",
        "body-parser": "1.20.2",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.1",
        "dotenv": "^16.4.5",
        "express": "4.19.2",
        "fs-extra": "11.2.0",
        "inversify": "6.0.2",
        "ioredis": "5.4.1",
        "jsonwebtoken": "9.0.2",
        "libphonenumber-js": "1.11.2",
        "morgan": "1.10.0",
        "multer": "1.4.5-lts.1",
        "reflect-metadata": "0.2.2",
        "uuid": "9.0.1",
        "winston": "3.13.0"
    },
    "devDependencies": {
        "@faker-js/faker": "8.4.1",
        "@types/express": "4.17.21",
        "@types/fs-extra": "11.0.4",
        "@types/jsonwebtoken": "9.0.6",
        "@types/morgan": "1.9.9",
        "@types/multer": "1.4.11",
        "@types/node": "20.12.12",
        "@types/uuid": "9.0.8",
        "@typescript-eslint/eslint-plugin": "6.21.0",
        "@typescript-eslint/parser": "6.21.0",
        "canvas": "2.11.2",
        "eslint": "8.57.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-import-resolver-typescript": "3.6.1",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-prettier": "5.1.3",
        "nodemon": "3.1.0",
        "prettier": "3.2.5",
        "ts-node": "10.9.2"
    }
}
